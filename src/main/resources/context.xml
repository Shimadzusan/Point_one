<?xml version="1.0" encoding="UTF-8"?>

<beans  xmlns="http://www.springframework.org/schema/beans"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xmlns:context="http://www.springframework.org/schema/context"
        xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context
        http://www.springframework.org/schema/context/spring-context.xsd">

   
    
    <bean id="simpleDay"
        class="Foto_Service.Point_one.Day">
        <property name="salary" value="10"/>
        
    </bean>
    
    <bean id="report_001"
        class="Foto_Service.Point_one.Config">
        <property name="deals" value="фнд,копия,печать,ником"/>
        
    </bean>
    
    <bean id="recognize"
          class="Foto_Service.Point_one.Recognize">
          <constructor-arg ref="simpleDay"/>
    </bean>

</beans>